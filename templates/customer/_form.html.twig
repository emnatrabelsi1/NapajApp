{{ form_start(form) }}

<div class="row">
    <div class="col-md-4">{{ form_row(form.name) }}</div>
</div>
<div class="row">
    <div class="col-md-4">{{ form_row(form.address) }}</div>
    <div class="col-md-4">{{ form_row(form.address2) }}</div>
</div>
<div class="row">
    <div class="col-md-2">{{ form_row(form.zipcode) }}</div>
    <div class="col-md-4">{{ form_row(form.city) }}</div>
    <div class="col-md-4">{{ form_row(form.route) }}</div>
</div>
<div class="row">
    <div class="col-md-6">{{ form_row(form.subscribed_tags) }}</div>
</div>
<div class="row">
    <div class="col-md-6">{{ form_row(form.internal) }}</div>
</div>

<button class="btn btn-success pull-right">{{ button_label|default('Enregistrer') }}</button>
{{ form_end(form) }}
